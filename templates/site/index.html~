    MagicThumb.options = {backgroundFadingOpacity : 0.8}
</script>
<script type="text/javascript"><!--搜索-->
function drop_mouseover(pos){
    try{window.clearTimeout(timer);}catch(e){}
}

function drop_mouseout(pos){
    var posSel=document.getElementById(pos+"Sel").style.display;
    if(posSel=="block"){
        timer = setTimeout("drop_hide('"+pos+"')", 1000);
    }
}

function drop_hide(pos){
    document.getElementById(pos+"Sel").style.display="none";
}

function search_show(pos,searchType,href){
    document.getElementById(pos+"SearchType").value=searchType;
    document.getElementById(pos+"Sel").style.display="none";
    document.getElementById(pos+"Slected").innerHTML=href.innerHTML;
    document.getElementById(pos+'q').focus();
    var sE = document.getElementById("searchytitle");
    if(sE != undefined  &&  searchType == "ytitle"){
        sE.style.display="block";
    }else if(sE != undefined){
        sE.style.display="none";
    }
    var sE = document.getElementById("searchltitle");
    if(sE != undefined  &&  searchType == "ltitle"){
        sE.style.display="block";
    }else if(sE != undefined){
        sE.style.display="none";
    }
    var sE = document.getElementById("searchbelite");
    if(sE != undefined  &&  searchType == "belite"){
        sE.style.display="block";
    }else if(sE != undefined){
        sE.style.display="none";
    }
    var sE = document.getElementById("searchnelite");
    if(sE != undefined  &&  searchType == "nelite"){
        sE.style.display="block";
    }else if(sE != undefined){
        sE.style.display="none";
    }
    try{window.clearTimeout(timer);}catch(e){}
    return false;
}
</script>

